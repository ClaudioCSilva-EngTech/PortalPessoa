  "version": "1.0.0",
  "description": "Portal Pessoas (RH e DP)",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "pm2 start ecosystem.config.cjs --name \"portal-quali-pessoas-dev\" --env development",
